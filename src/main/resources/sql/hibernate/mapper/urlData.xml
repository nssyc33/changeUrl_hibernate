<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="testGitHub.io.Entity.UrlData" table="tb_url">
        <id name="id" type="string">
            <column name="ID" not-null="true" />
        </id> 
        <property name="oriUrl" type="string">
            <column name="ORI_URL" length="30" not-null="false" />
        </property>
        <property name="subUrl" type="string">
            <column name="SUB_URL" length="30" not-null="false" />
        </property>
        <property name="subKey" type="string">
            <column name="SUB_KEY" length="15" not-null="false" />
        </property>
    </class>
</hibernate-mapping>